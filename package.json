        "npx eslint --fix"
      ]
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-anderson/book-nest.git"
  },
  "keywords": [
    "book",
    "nest"
  ],
  "author": "Helen, Muhamad, Marko, Sushan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-anderson/book-nest/issues"
  },
  "homepage": "https://github.com/grace-anderson/book-nest/",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-session-sequelize": "^7.1.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "mysql2": "^2.3.3",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
